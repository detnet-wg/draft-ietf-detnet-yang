{
  "ietf-interfaces:interfaces": {
    "interface": [
      {
        "name": "eth0"
      },
      {
        "name": "eth1"
      },
      {
        "name": "eth2"
      },
      {
        "name": "eth3"
      },
      {
        "name": "eth4"
      }
    ]
  },
  "ietf-detnet:detnet": {
    "traffic-profile": [
      {
        "profile-name": "pf-1",
        "traffic-specification": {
          "interval": 125,
          "max-packets-per-interval": 1,
          "max-payload-size": 1518
        },
        "member-forwarding-sublayers": [
          "fsl-1",
          "fsl-2"
        ]
      },
      {
        "profile-name": "pf-2",
        "traffic-specification": {
          "interval": 125,
          "max-packets-per-interval": 2,
          "max-payload-size": 1518
        },
        "member-forwarding-sublayers": [
          "afl-1"
        ]
      }
    ],
    "forwarding-sub-layer": {
      "forwarding-sub-layer-list": [
        {
          "name": "fsl-1",
          "traffic-profile": "pf-1",
          "forwarding-operation-type": "pop-impose-and-forward",
          "incoming-type": {
            "interface": "eth0",
            "mpls-label-stack": {
              "entry": [
                {
                  "id": 0,
                  "label": 10000
                }
              ]
            }
          },
          "outgoing-type": {
            "aggregation-forwarding-sub-layer": "afl-1",
            "forwarding-label": {
              "mpls-label-stack": {
                "entry": [
                  {
                    "id": 0,
                    "label": 10002
                  }
                ]
              }
            }
          }
        },
        {
          "name": "fsl-2",
          "traffic-profile": "pf-1",
          "forwarding-operation-type": "pop-impose-and-forward",
          "incoming-type": {
            "interface": "eth1",
            "mpls-label-stack": {
              "entry": [
                {
                  "id": 0,
                  "label": 10004
                }
              ]
            }
          },
          "outgoing-type": {
            "aggregation-forwarding-sub-layer": "afl-1",
            "forwarding-label": {
              "mpls-label-stack": {
                "entry": [
                  {
                    "id": 0,
                    "label": 10006
                  }
                ]
              }
            }
          }
        },
        {
          "name": "afl-1",
          "traffic-profile": "pf-2",
          "forwarding-operation-type": "impose-and-forward",
          "incoming-type": {
            "forwarding-sub-layer": [
              "fsl-1",
              "fsl-2"
            ]
          },
          "outgoing-type": {
            "outgoing-interface": "eth3",
            "mpls-label-stack": {
              "entry": [
                {
                  "id": 0,
                  "label": 20000
                }
              ]
            }
          }
        }
      ]
    }
  }
}

