<detnet
  xmlns="urn:ietf:params:xml:ns:yang:ietf-detnet"
  xmlns:in="urn:ietf:params:xml:ns:yang:ietf-inet-types"
  xmlns:rt="urn:ietf:params:xml:ns:yang:ietf-routing-types"
  xmlns:if="urn:ietf:params:xml:ns:yang:ietf-interfaces"
  xmlns:r="urn:ietf:params:xml:ns:yang:ietf-routing"
  xmlns:mpls="urn:ietf:params:xml:ns:yang:ietf-mpls"
  xmlns:pf="urn:ietf:params:xml:ns:yang:ietf-packet-fields">
  <traffic-profile>  
    <name>pf-1</name>
    <traffic-spec>
      <interval>125</interval>
      <max-pkts-per-interval>1</max-pkts-per-interval>
      <max-payload-size>1518</max-payload-size>
    </traffic-spec>
    <member-fwd-sublayers>fsl-1</member-fwd-sublayers>
    <member-fwd-sublayers>fsl-2</member-fwd-sublayers>
  </traffic-profile>  
  <traffic-profile>  
    <name>pf-2</name>
    <traffic-spec>
      <interval>125</interval>
      <max-pkts-per-interval>2</max-pkts-per-interval>
      <max-payload-size>1518</max-payload-size>
    </traffic-spec>
    <member-fwd-sublayers>afl-1</member-fwd-sublayers>
  </traffic-profile>
  <forwarding>
    <sub-layer>
      <name>fsl-1</name>
      <traffic-profile>pf-1</traffic-profile>
      <operation>pop-impose-and-forward</operation>
      <incoming>
        <forwarding-id>
          <interface>eth0</interface>
          <mpls-label-stack>
            <entry>
              <id>0</id>
              <label>10000</label>
            </entry>
          </mpls-label-stack>
        </forwarding-id>
      </incoming>
      <outgoing>
        <forwarding-sub-layer>
          <aggregation-sub-layer>afl-1</aggregation-sub-layer>
          <forwarding-label>       
            <mpls-label-stack>
              <entry>
                <id>0</id>
                <label>10002</label>
              </entry>
            </mpls-label-stack>
          </forwarding-label>       
        </forwarding-sub-layer>
      </outgoing>
    </sub-layer>
    <sub-layer>
      <name>fsl-2</name>
      <traffic-profile>pf-1</traffic-profile>
      <operation>pop-impose-and-forward</operation>
      <incoming>
        <forwarding-id>
          <interface>eth1</interface>
          <mpls-label-stack>
            <entry>
              <id>0</id>
              <label>10004</label>
            </entry>
          </mpls-label-stack>
        </forwarding-id>
      </incoming>
      <outgoing>
        <forwarding-sub-layer>
          <aggregation-sub-layer>afl-1</aggregation-sub-layer>
          <forwarding-label>
            <mpls-label-stack>
              <entry>
                <id>0</id>
                <label>10006</label>
              </entry>
            </mpls-label-stack>
          </forwarding-label>
        </forwarding-sub-layer>
      </outgoing>
    </sub-layer>
    <sub-layer>
      <name>afl-1</name>
      <traffic-profile>pf-2</traffic-profile>
      <operation>impose-and-forward</operation>
      <incoming>
        <forwarding-aggregation>
          <sub-layer>fsl-1</sub-layer>
          <sub-layer>fsl-2</sub-layer>
        </forwarding-aggregation>
      </incoming>
      <outgoing>
        <interface>
          <outgoing-interface>eth3</outgoing-interface>
          <mpls-label-stack>
            <entry>
              <id>0</id>
              <label>20000</label>
            </entry>
          </mpls-label-stack>
        </interface>
      </outgoing>
    </sub-layer>
  </forwarding>
</detnet>
